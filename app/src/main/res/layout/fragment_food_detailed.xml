<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.FoodDetailedFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp">

        <TextView
            android:id="@+id/foodName_detailed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="19sp"
            android:textStyle="bold"
            android:layout_marginTop="16dp"
            android:text="Food Name"
            android:layout_marginStart="16dp" />

        <TextView
            android:id="@+id/foodGroup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:textStyle="bold"
            android:layout_marginTop="12dp"
            android:text="Category"
            android:layout_below="@+id/foodName_detailed"
            android:layout_marginStart="16dp" />


        <TextView
            android:id="@+id/food_protein_detailed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:textStyle="bold"
            android:text="Proteina:"
            android:layout_below="@+id/foodGroup"
            android:layout_marginTop="12dp"
            android:layout_marginStart="16dp"/>


        <TextView
            android:id="@+id/foodProtein_detailed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:layout_toEndOf="@+id/food_protein_detailed"
            android:text="Protein number"
            android:layout_alignTop="@+id/food_protein_detailed"
            android:layout_marginStart="8dp"/>

        <TextView
            android:id="@+id/food_carbs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:textStyle="bold"
            android:text="Carbohidratos:"
            android:layout_below="@+id/food_protein_detailed"
            android:layout_marginTop="12dp"
            android:layout_marginStart="16dp"/>

        <TextView
            android:id="@+id/foodcarbs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:layout_toEndOf="@+id/food_carbs"
            android:text="Carbs number"
            android:layout_alignTop="@+id/food_carbs"
            android:layout_marginStart="8dp"/>

        <TextView
            android:id="@+id/food_fiber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:textStyle="bold"
            android:text="Fibra:"
            android:layout_below="@+id/food_carbs"
            android:layout_marginTop="12dp"
            android:layout_marginStart="16dp"/>

        <TextView
