            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:layout_toEndOf="@+id/food_potasio"
            android:text="Potasio number"
            android:layout_alignTop="@+id/food_potasio"
            android:layout_marginStart="8dp"/>

        <TextView
            android:id="@+id/food_vitaminaa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:textStyle="bold"
            android:text="Vitamina A:"
            android:layout_below="@+id/food_potasio"
            android:layout_marginTop="12dp"
            android:layout_marginStart="16dp"/>

        <TextView
            android:id="@+id/foodvitaminaa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
